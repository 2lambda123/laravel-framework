{
    "name": "illuminate/markdown",
    "description": "The Illuminate Markdown package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/container": "^7.0",
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Markdown\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "suggest": {
        "erusev/parsedown": "Required to use the Parsedown renderer (^1.7).",
        "league/commonmark": "Required to use the CommonMark renderer (^1.9).",
        "michelf/php-markdown": "Required to use the PHP Markdown renderer (^1.8)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
